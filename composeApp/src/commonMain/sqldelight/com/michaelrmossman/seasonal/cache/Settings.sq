
CREATE TABLE Settings (
    setting TEXT NOT NULL PRIMARY KEY,
    value TEXT NOT NULL
);

deleteSettings:
DELETE FROM Settings;

getSetting:
SELECT * FROM Settings
WHERE setting = ?;

getSettings:
SELECT * FROM Settings;

insertSetting:
INSERT OR REPLACE INTO Settings(setting, value)
VALUES (?,?);

