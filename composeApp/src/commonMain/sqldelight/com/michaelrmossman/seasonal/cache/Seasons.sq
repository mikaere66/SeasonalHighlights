
/**
 * 
 */
CREATE TABLE Seasons(
    seId INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    code TEXT NOT NULL
);

getSeason:
SELECT * FROM Seasons
WHERE code = ?;

getSeasons:
SELECT * FROM Seasons;

insertSeason:
INSERT INTO Seasons(seId, code)
VALUES (?,?);

