    {
      "type": "Feature",
      "geometry": {
        "type": "Point",
        "coordinates": [
          172.626906,
          -43.531484
        ]
      },
      "properties": {
        "SeasonalName": "Peacock Fountain",
        "SeasonalCode": "Seasonal Highlights Late Summer",
        "Status": "November to February: 7.00am to 9.00pm.",
        "Description": "Set in the picturesque Armstrong Lawn, the Peacock Fountain was purchased from the famous Coalbrookdale Foundry in Shropshire, England and erected in 1911. It moved location twice before being dismantled in 1949. Public support called for its restoration and it was recommissioned in 1996 after recasting, cleaning, strengthening and painting the pieces. This iconic fountain has long been a contentious piece in the art world with its colours and form often criticised but it is nonetheless seen by many Cantabrians as an integral part of the Botanic Gardens.",
        "Photo": "https://ccc.govt.nz/assets/Images/Culture-Community/Heritage/_resampled/ResizedImageWzgwMCw1NzJd/Peacock-Fountain-May2012-0752resize.jpg"
      }
